{
  "name": "news-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "mocha ./tests --timeout 10000 --exit",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "nyc": {
    "register": "babel-register",
    "include": [
      "**/*.js",
      "./*.js"
    ],
    "exclude": [
      "**/node_modules/**",
      "**/tests/**",
      "**/coverage/**"
    ],
    "all": true
  },
  "dependencies": {
    "async-middleware": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "~4.16.0",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "http-errors": "~1.6.2",
    "mongoose": "^5.4.7",
    "mongoose-find-or-create": "^1.3.1",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "passport-vkontakte": "^0.3.2",
    "pug": "2.0.0-beta11",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0"
  }
}
